polarXY <- function(x,y) {
r <- sqrt(x^2 + y^2)
theta <- atan(y/x)
theta[abs(theta) == Inf, ] <- 0
theta[theta == NaN,] <- 0
data.frame(radius=r,theta=theta)
}
